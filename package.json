{
  "name": "workout-helper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "now": "now --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@tailwindcss/ui": "0.5.0",
    "@testing-library/jest-dom": "5.11.2",
    "@testing-library/react": "10.4.7",
    "@types/jest": "26.0.8",
    "@types/node": "14.0.27",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.0",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "@xstate/react": "0.8.1",
    "acd-utils": "7.1.0",
    "autoprefixer": "9.8.6",
    "await-to-js": "2.1.1",
    "babel": "6.23.0",
    "classcat": "4.1.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.2.2",
    "jest-watch-typeahead": "0.6.0",
    "lint-staged": "10.2.11",
    "next": "9.5.1",
    "nosleep.js": "0.11.0",
    "now": "19.2.0",
    "nprogress": "0.2.0",
    "postcss-import": "12.0.1",
    "postcss-nesting": "7.0.1",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-transition-group": "4.4.1",
    "stylelint": "13.6.1",
    "stylelint-config-concentric-order": "4.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "tailwindcss": "1.6.2",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "xstate": "4.11.0"
  }
}
