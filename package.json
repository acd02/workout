{
  "name": "workout-helper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build:static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:styles": "stylelint --fix \"./src/**/*.module.css\"",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "vercel": "vercel --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.10",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@xstate/react": "1.0.1",
    "acd-utils": "7.1.0",
    "autoprefixer": "9.8.6",
    "await-to-js": "2.1.1",
    "babel": "6.23.0",
    "classcat": "4.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.5.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.4.2",
    "next": "9.5.5",
    "nprogress": "0.2.0",
    "postcss-import": "12.0.1",
    "postcss-nesting": "7.0.1",
    "prettier": "2.1.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-test-renderer": "16.14.0",
    "react-transition-group": "4.4.1",
    "stylelint": "13.7.2",
    "stylelint-config-concentric-order": "4.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "tailwindcss": "1.9.3",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "vercel": "20.1.2",
    "xstate": "4.13.0"
  }
}
